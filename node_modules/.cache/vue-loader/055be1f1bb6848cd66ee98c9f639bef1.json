 month = \"Jun\";\r\n      } else if (month == \"07\") {\r\n        month = \"Jul\";\r\n      } else if (month == \"08\") {\r\n        month = \"Aug\";\r\n      } else if (month == \"09\") {\r\n        month = \"Sep\";\r\n      } else if (month == \"10\") {\r\n        month = \"Okt\";\r\n      } else if (month == \"11\") {\r\n        month = \"Nov\";\r\n      } else if (month == \"12\") {\r\n        month = \"Des\";\r\n      }\r\n\r\n      return `${month}`;\r\n    }\r\n  },\r\n  methods: {\r\n    getArticle: function() {\r\n      this.axios\r\n        .get(\"/artikel?kategori=Berita\")\r\n        .then(response => {\r\n          this.items = response.data.data;\r\n          this.items.sort((a, b) => b.id - a.id);\r\n        })\r\n        .catch(err => {\r\n          // console.log(err);\r\n        });\r\n    },\r\n    getAgenda: function() {\r\n      this.axios\r\n        .get(\"/artikel?kategori=Event\")\r\n        .then(response => {\r\n          this.agenda = response.data.data;\r\n          this.agenda.sort((a, b) => b.id - a.id);\r\n          // for (let i = 0; i < this.agenda.length; i++) {\r\n          //   let tgl = this.agenda[i].date.substring(5, 7);\r\n          //   if (tgl == \"01\") {\r\n          //     tgl = \"Jan\";\r\n          //   } else if (tgl == \"02\") {\r\n          //     tgl = \"Feb\";\r\n          //   } else if (tgl == \"03\") {\r\n          //     tgl = \"Mar\";\r\n          //   } else if (tgl == \"04\") {\r\n          //     tgl = \"Apr\";\r\n          //   } else if (tgl == \"05\") {\r\n          //     tgl = \"Mei\";\r\n          //   } else if (tgl == \"06\") {\r\n          //     tgl = \"Jun\";\r\n          //   } else if (tgl == \"07\") {\r\n          //     tgl = \"Jul\";\r\n          //   } else if (tgl == \"08\") {\r\n          //     tgl = \"Aug\";\r\n          //   } else if (tgl == \"09\") {\r\n          //     tgl = \"Sep\";\r\n          //   } else if (tgl == \"10\") {\r\n          //     tgl = \"Okt\";\r\n          //  